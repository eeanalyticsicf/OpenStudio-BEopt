  <class_name>ProcessThermalMassPartitionWall</class_name>
  <display_name>ProcessThermalMassPartitionWall</display_name>
  <description>This measure creates internal mass for partition walls in the living space and finished basement.</description>
  <modeler_description>This measure creates constructions representing the internal mass of partition walls in the living space and finished basement. The constructions are set to define the internal mass objects of their respective spaces.</modeler_description>
  <arguments>
    <argument>
      <name>selectedliving</name>
      <display_name>Of what space type is the living space?</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>selectedfbsmt</name>
      <display_name>Of what space type is the finished basement?</display_name>
      <type>Choice</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>userdefinedpartitionwallmassth</name>
      <display_name>Partition wall mass thickness [in].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.5</default_value>
    </argument>
    <argument>
      <name>userdefinedpartitionwallmasscond</name>
      <display_name>Partition wall mass conductivity [Btu-in/h-ft^2-R].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>1.1112</default_value>
    </argument>
    <argument>
      <name>userdefinedpartitionwallmassdens</name>
      <display_name>Partition wall mass density [lb/ft^3].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>50</default_value>
    </argument>
    <argument>
      <name>userdefinedpartitionwallmasssh</name>
      <display_name>Partition wall mass specific heat [Btu/lb-R].</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>0.2</default_value>
    </argument>
    <argument>
      <name>userdefinedpartitionwallfrac</name>
      <display_name>Ratio of exposed partition wall area to total conditioned floor area and accounts for the area of both sides of partition walls.</display_name>
      <type>Double</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>userdefinedlivingarea</name>
      <display_name>The area of the living space [ft^2].</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>2700</default_value>
    </argument>
    <argument>
      <name>userdefinedfbsmtarea</name>
      <display_name>The area of the finished basement [ft^2].</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1200</default_value>
    </argument>
  </arguments>
