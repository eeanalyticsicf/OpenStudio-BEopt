  <class_name>ResidentialMiscellaneousGasLoads</class_name>
  <display_name>ResidentialMiscellaneousGasLoads</display_name>
  <description>This measure adds miscellaneous gas loads (representing a pool heater, spa heater, fireplace, grill, and/or lighting) to the living space. Users can specify the annual gas consumption (in therms) of their MGL or a faction of the benchmark MGLs to add to the space.</description>
  <modeler_description>This measure creates a new gas equipment object and adds it to the user selected space. If MGLs are already present in the model, this measure will replace them with the user specified MGL.</modeler_description>
  <arguments>
    <argument>
      <name>selected_mgl</name>
      <display_name>MGL Energy Consumption Option</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <choices>
        <choice>
          <value>Benchmark</value>
          <display_name>Benchmark</display_name>
        </choice>
        <choice>
          <value>Simple</value>
          <display_name>Simple</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>mgl_E</name>
      <display_name>Simple MGL Annual Energy Consumption (therm/yr)</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>CondFA</name>
      <display_name>Living Space Floor Area (ft^2)</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1800</default_value>
    </argument>
    <argument>
      <name>BAB_mult</name>
      <display_name>Building America Benchmark Multipler</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>Num_Br</name>
      <display_name>Number of Bedrooms</display_name>
      <type>Integer</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>space_type</name>
      <display_name>Select the space where the miscellaneous gas loads are located</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
  </arguments>
